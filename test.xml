<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="AutomationPracticeTest" parallel="methods"
	thread-count="1" verbose="1">
	<test name="UI Automation Test">

		<parameter name="browserName" value="${browserName}" />
		<parameter name="isHeadless" value="${isHeadless}" />
		<parameter name="isLambdaTest" value="${isLambdaTest}" />

		<classes>
			<class name="com.ui.test.LoginTest" />
			<class name="com.ui.test.AddNewAddressTest" />
			<class name="com.ui.test.InvalidCredLoginTest" />
			<class name="com.ui.test.ProductCheckoutTest" />
			<class name="com.ui.test.ProductSearchTest" />

		</classes>
	</test>


</suite>